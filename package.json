{
  "name": "create-ak-app",
  "version": "1.0.50",
  "description": "CLI for nextjs based rendering template skeleton project.",
  "bin":  "dist-esm/index.js",
  "type": "module",
  "main": "dist-cjs/index.js",
  "module": "dist-esm/index.js",
  "exports": {
    "import": "./dist-esm/index.js",
    "require": "./dist-cjs/index.js"
  },
  "scripts": {
    "copy-assets": "node --experimental-json-modules ./bundler-helpers/copy-assets.js",

    "debug": "node --inspect-brk --experimental-json-modules ./dist-esm/index.js my-next-app --use-npm",
    "create": "node --experimental-json-modules ./dist-esm/index.js my-next-app",
    "create:dynamic": "node --experimental-json-modules ./dist-esm/index.js ",

    "clean": "rimraf ./dist-esm ./dist-cjs",

    "tsc": "tsc  --resolveJsonModule",
    "tsc-cjs": "tsc --p tsconfig-cjs.json --resolveJsonModule",
    "prerelease": "npm run clean",
    "release": "npm-run-all -s tsc copy-assets",
    "prepublish": "npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kolusamkhan/create-ak-app.git"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kolusamkhan/create-ak-app/issues"
  },
  "homepage": "https://github.com/kolusamkhan/create-ak-app#readme",
  "dependencies": {
    "@types/async-retry": "^1.4.4",
    "@types/cross-spawn": "^6.0.2",
    "@types/load-json-file": "^5.1.0",
    "@types/node": "^17.0.32",
    "@types/prompts": "^2.0.14",
    "@types/rimraf": "^3.0.2",
    "@types/tar": "^6.1.1",
    "@types/validate-npm-package-name": "3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "async-retry": "^1.3.3",
    "auto-changelog": "^2.4.0",
    "chalk": "^5.0.1",
    "commander": "^9.2.0",
    "cpy": "7.3.0",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "got": "^12.0.4",
    "jest": "^28.1.0",
    "load-json-file": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "tar": "^6.1.11",
    "typedoc": "^0.22.15",
    "typescript": "^4.4.3",
    "update-check": "^1.5.4",
    "validate-npm-package-name": "3.0.0"
  },
  "bundleDependencies":true
}
